#include <stdio.h>
/**
 * main - Patience, persistence and perspiration make an unbeatable combination for success 
 *
 * Return: int
 **/
int main(void)
{
  int n, k;
  for(n = 0; n <= 9; n++)
    {
      if(n <= 8){
	
	putchar('0' + n);
	putchar(',');
	putchar(' ');
      }
(n == 9){
putchar(n + '0');
}
}
return 0;
}